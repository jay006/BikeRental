<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.shriom.bikerental">

    <application
        android:name=".BikeRental"
        android:allowBackup="true"
        android:icon="@drawable/bike_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bike_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">


        <activity android:name=".ui.MasterActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.shriom.bikerental.ui.home.HomeActivity">
        </activity>
        <activity android:name=".ui.onboard.OnBoardActivity" />
        <activity android:name=".ui.search.SearchActivity" />
        <activity android:name=".ui.checkout.CheckOutActivity" />


    </application>

</manifest>